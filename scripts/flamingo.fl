class ListReplacement(List) {
    @meta fun init(vararg items) {
        self.items = items
    }

    @meta fun displayObject() = "ListReplacement[{self.items.size()}]"
    @meta fun isIterable() = true
    @meta fun iter() = ListRepIter(self)

}

class ListRepIter {
    @meta fun init(listReplacement) {
        self.listReplacement = listReplacement
        self.pos = 0
    }

    fun hasNextObj() = self.pos < self.listReplacement.items.size()
    fun nextObj() {
        item = self.listReplacement.items[self.pos]
        pos = self.pos
        self.pos = self.pos + 1
        return (self.pos)
    }
}


val l = filter(ListReplacement, [1, 2, 3, 4, 5, 6, 6]) { it.isEven() }

