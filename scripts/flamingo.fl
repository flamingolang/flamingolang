class A {
    @meta fun init(a) {
        self.a = a
    }

    @meta fun displayObject() = "{self.getClass().getName()}({self.a})"

    fun hello() = "one"
}


class B(A) {
    @meta fun init(a) {
        super(A, a)
    }

    fun hello() = super.hello()
}

println(B(1).a)
val b = B(1)
println(b)
println(b.hello())